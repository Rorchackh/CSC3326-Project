        </Field>
        <Field Name="CUS_LNAME">
          <DataField>CUS_LNAME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CUS_FNAME">
          <DataField>CUS_FNAME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>khobalatte_a</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT     p.PRO_DESCRIPT, SUM(r.REV_RATE) AS total_rate, p.PRO_NAME, c.CAT_NAME, r.REV_DATE, r.REV_RATE, r.REV_TITLE, r.REV_TEXT, cu.CUS_LNAME, 
                      cu.CUS_FNAME
FROM         PRODUCT AS p INNER JOIN
                      REVIEW AS r ON p.PRO_ID = r.PRO_ID INNER JOIN
                      CUSTOMER AS cu ON r.CUS_ID = cu.CUS_ID INNER JOIN
                      VENDOR AS v ON p.VED_ID = v.VED_ID INNER JOIN
                      CATEGORY AS c ON p.CAT_ID = c.CAT_ID
GROUP BY p.PRO_DESCRIPT, p.PRO_NAME, c.CAT_NAME, r.REV_DATE, r.REV_RATE, r.REV_TITLE, r.REV_TEXT, cu.CUS_LNAME, cu.CUS_FNAME</CommandText>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
