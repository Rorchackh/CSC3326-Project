      </Fields>
      <Query>
        <DataSourceName>khobalatte_a</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT     p.PRO_DESCRIPT, SUM(r.rev_rate) as total_rate, p.PRO_NAME, c.CAT_NAME, r.REV_DATE, r.REV_RATE, r.REV_TITLE, 
                      r.REV_TEXT, cu.CUS_LNAME, cu.CUS_FNAME
FROM         product  p INNER JOIN
                      review  r ON p.PRO_ID = r.PRO_ID INNER JOIN
                      customer  cu ON r.CUS_ID = cu.CUS_ID INNER JOIN
                      vendor  v ON p.VED_ID = v.VED_ID INNER JOIN
                      category  c ON p.CAT_ID = c.CAT_ID
group by p.PRO_DESCRIPT, p.PRO_NAME, c.CAT_NAME, r.REV_DATE, r.REV_RATE, r.REV_TITLE, 
                      r.REV_TEXT, cu.CUS_LNAME, cu.CUS_FNAME
order by total_rate</CommandText>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
